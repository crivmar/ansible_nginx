# nginx
virtualhosts:
  - name: vhost1
    datos:
      nameserver: vhost1.org
      documentroot: /var/www/vhost1
      errorlog: error_vhost1
      accesslog: access_vhost1
  - name: vhost2
    datos:
      nameserver: vhost2.org
      documentroot: /var/www/vhost2
      errorlog: error_vhost2
      accesslog: access_vhost2



